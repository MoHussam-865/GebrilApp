<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/mainFragment3">
    <fragment
        android:id="@+id/itemsChooseFragment"
        android:name="com.android_a865.gebril_app.feature_main.presentation.items_choose.ItemsChooseFragment"
        tools:layout="@layout/fragment_items_choose">
        <action
            android:id="@+id/action_itemsChooseFragment_to_newEstimateFragment"
            app:destination="@id/newEstimateFragment" />
    </fragment>
    <fragment
        android:id="@+id/newEstimateFragment"
        android:name="com.android_a865.gebril_app.feature_main.presentation.confirmation.ConfirmationFragment"
        tools:layout="@layout/fragment_confirm">
        <argument
            android:name="items"
            app:argType="com.android_a865.gebril_app.data.domain.InvoiceItem[]" />
        <action
            android:id="@+id/action_newEstimateFragment_to_viewPdfFragment"
            app:destination="@id/viewPdfFragment" />
        <argument
            android:name="is_created"
            android:defaultValue="true"
            app:argType="boolean" />
    </fragment>
    <fragment
        android:id="@+id/viewPdfFragment"
        android:name="com.android_a865.gebril_app.feature_main.presentation.pdf_preview.ViewPdfFragment"
        tools:layout="@layout/fragment_view_pdf">
        <argument
            android:name="items"
            app:argType="com.android_a865.gebril_app.data.domain.InvoiceItem[]" />
        <action
            android:id="@+id/action_viewPdfFragment_to_mainFragment3"
            app:destination="@id/mainFragment3" />
    </fragment>
    <fragment
        android:id="@+id/invoicesViewFragment"
        android:name="com.android_a865.gebril_app.feature_main.presentation.invoices_view.InvoicesViewFragment"
        tools:layout="@layout/fragment_invoices_view" />
    <fragment
        android:id="@+id/mainFragment3"
        android:name="com.android_a865.gebril_app.feature_main.presentation.main_page.MainFragment"
        android:label="@string/app_name"
        tools:layout="@layout/fragment_main">
        <action
            android:id="@+id/action_mainFragment3_to_itemsChooseFragment"
            app:destination="@id/itemsChooseFragment" />
        <action
            android:id="@+id/action_mainFragment3_to_invoicesViewFragment"
            app:destination="@id/invoicesViewFragment" />
    </fragment>
</navigation>